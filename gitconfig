# Include this file from ~/.gitconfig:
# [include]
#     path = /path/to/this/file
[color]
	ui = auto
[diff]
	renames = copy
	renameLimit = 512
[core]
	pager = less -+$LESS -isR
	quotepath = off
[push]
	default = simple
[gc]
	auto = 32768
	autopacklimit = 256
[alias]
	fgrep = grep -F
	egrep = grep -E
	pgrep = grep -P
