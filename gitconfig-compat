# Include this file from ~/.gitconfig:
# [include]
#     path = /path/to/this/file
[color]
	ui = auto
[diff]
	renames = copy
	renameLimit = 512
[core]
	pager = less -+$LESS -isR
	quotepath = off
[gc]
	auto = 16384
	autopacklimit = 128
[alias]
	fgrep = grep -F
	egrep = grep -E
	pgrep = grep -P
[fetch]
	prune = true
